package com.Bridgelabz.Functional;

import java.util.Scanner;

public class Matrix {
	public static void mian(String[] args) {
	
		Matrix m=new Matrix();
		m.matrix();
	}
	public  int matrix() {
		int a[][]=new int[3][3];
	
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the elements:");
		int n=s.nextInt();
		System.out.println("Enter the elements:");

		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				a[i][j]=s.nextInt();
			}
		}
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				System.out.println(a[i][j]+" ");
				
			}
			System.out.println();
		}
		return n;
	
	}
}
