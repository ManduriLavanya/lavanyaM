package com.Bridgelabz.Algorithms;

import java.util.Scanner;

public class Prime {

		public static void main(String[] args) {
			Scanner s=new Scanner(System.in);
			System.out.println("Enter prime numbers of certain range for a:");
			int a=s.nextInt();
			System.out.println("Enter prime numbers of certain range for b:");
			int b=s.nextInt();
			int flag=0;
			for(int i = a; i <= b; i++)
	        {
	            for( int j = 2; j < i; j++)
	            {
	                if(i % j == 0)
	                {
	                    flag = 0;
	                    break;
	                }
	                else
	                {
	                    flag = 1;
	                }
	            }
	            if(flag == 1)
	            { 

	               System.out.println(i);
	            }
	        }
		}

}
