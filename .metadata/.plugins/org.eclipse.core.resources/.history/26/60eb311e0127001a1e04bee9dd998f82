package com.Bridgelabz.Generics;

import java.util.Scanner;

public class Bst {

	public static void main(String[] args) {
		System.out.println("Enter thr n number:");
		Scanner s=new Scanner(System.in);
		
		int n=s.nextInt();
		int i;
		
		int a[]=new int [1000];
		a[1]=1;a[0]=1;
		for( i=2;i<=n;i++) {
			//a[i]=0;
			for(int j=0;j<i;j++) {
				a[i]+=a[j]*a[i-j-1];
			}

		}
		System.out.println(a[i]);

	}

}
