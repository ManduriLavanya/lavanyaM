package com.Bridgelabz.Generics;

public class PrimeArray {

	public static void main(String[] args) {
		
		int[] arr=new int[1000];
		int l=0;
		int r=10,c=10;
				for (int n=0;n<1000;n++)
					{
				boolean a=isPrime(n);
				
			
			
			
			for(int i=0;i<r;i++) {
				for(int j=0;j<c;j++) {
					if (a==true) 
					{
					arr[l]=n;
					System.out.print(arr[l]+" ");
					l++;
					}
				}
			}
			}
	}
	static  boolean isPrime(int n)
    { 
        if (n <= 1) 
            return false; 
       
   	        for (int i = 2; i < n; i++) 
            if (n % i == 0) 
                return false;  
        return true;
    } 

}
